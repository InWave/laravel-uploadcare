{
    "name": "inwave/laravel-uploadcare",
    "description": "Laravel Service for Uploadcare",
    "keywords": ["laravel", "uploadcare"],
    "homepage":"https://github.com/InWave/laravel-uploadcare.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Overtoom",
            "email": "kevin@inwave.nl",
            "homepage": "http://www.inwave.nl",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/uploadcare/uploadcare-php.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": ">=5.4.0",
        "laravelcollective/html": ">=5.4.0",
        "uploadcare/uploadcare-php": ">=v1.1.0"
    },
    "autoload": {
        "psr-0": {
            "Inwave\\LaravelUploadcare": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Illuminate\\Html\\HtmlServiceProvider::class",
                "Inwave\\LaravelUploadcare\\LaravelUploadcareServiceProvider::class",
            ],
            "aliases": {
                    "Form": "Illuminate\\Html\\FormFacade::class",
                    "HTML": "Illuminate\\Html\\HtmlFacade::class",
                    "Uploadcare": "Inwave\\LaravelUploadcare\\Facades\\Uploadcare::class"
            }
        }
    }
}
